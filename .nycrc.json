{
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "extension": ".ts",
    "exclude": [
        "test/**/*"
    ],
    "include": [
        "src/**/*"
    ],
    "reporter": [
        "text-summary",
        "text"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
}